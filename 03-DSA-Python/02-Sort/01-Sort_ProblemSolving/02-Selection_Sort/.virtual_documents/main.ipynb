# selection sort

def selection_sort(l):
    n = len(l)
    for i in range(n-1):
        min_index = i
        for j in range(i+1,n):
            if l[j] < l[min_index]:
                min_index = j
        if i != min_index:
            l[i], l[min_index] = l[min_index],l[i]
    return l

l = [4,3,12,1]
print(selection_sort(l))









